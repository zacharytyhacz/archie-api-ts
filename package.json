{
  "name": "archie-api-ts",
  "version": "0.1.0",
  "description": "TypeScript wrapper for the Archie API",
  "license": "GPL-3.0-or-later",
  "author": "",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --fix",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "archie",
    "api",
    "sdk",
    "typescript"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "dotenv": "^17.3.1",
    "eslint": "^9.39.2",
    "eslint-plugin-unused-imports": "^4.4.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.55.0"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "ts-node": "^10.9.2"
  }
}
